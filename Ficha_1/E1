package exercicios;

public class E3 {
	
	 static int horasParaSegundos(int horas, int minutos, int segundos) {
			int horasParaSegundos = horas*3600;
			int minutosParaSegundos = minutos * 60;
			int ConversaoTotal = horasParaSegundos + minutosParaSegundos + segundos;
			return ConversaoTotal;
		}
	 
	public static void main(String[] args) {
		int resultado = horasParaSegundos(3, 0, 0) - horasParaSegundos(1, 0, 0);
		System.out.println(resultado);

	}

}
